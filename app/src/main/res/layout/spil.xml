<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Spil_akt">

    <ImageView
        android:id="@+id/galge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/galge"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/forkert1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:src="@drawable/forkert1"
        app:layout_constraintStart_toStartOf="@+id/galge"
        app:layout_constraintTop_toTopOf="@+id/galge" />

    <ImageView
        android:id="@+id/forkert2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/forkert2"
        app:layout_constraintStart_toStartOf="@+id/galge"
        app:layout_constraintTop_toTopOf="@+id/forkert1" />

    <ImageView
        android:id="@+id/forkert3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/forkert3"
        app:layout_constraintStart_toStartOf="@+id/galge"
        app:layout_constraintTop_toTopOf="@+id/galge" />

    <ImageView
        android:id="@+id/forkert5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="forkert5"
        android:src="@drawable/forkert6"
        app:layout_constraintStart_toStartOf="@+id/galge"
        app:layout_constraintTop_toTopOf="@+id/forkert1" />

    <ImageView
        android:id="@+id/forkert4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/forkert5"
        app:layout_constraintStart_toStartOf="@+id/galge"
        app:layout_constraintTop_toTopOf="@+id/forkert1" />

    <TextView
        android:id="@+id/info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="113dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="556dp" />
</androidx.constraintlayout.widget.ConstraintLayout>